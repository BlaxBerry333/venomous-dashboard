{
  "name": "venomous-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e envs/.env.development next dev --turbopack",
    "start": "dotenv -e envs/.env.production next start",
    "build": "dotenv -e envs/.env.production next build --turbopack",
    "check:packages": "npx depcheck",
    "check:type": "tsc --noEmit --incremental --tsBuildInfoFile ./.cache/.tsbuildinfo",
    "check:eslint": "eslint . --cache --cache-location=./.cache/.eslintcache",
    "check:prettier": "prettier . --check --cache --cache-location=./.cache/.prettiercache",
    "check:cspell": "cspell --cache --cache-location=./.cache/.cspellcache --no-progress --show-suggestions '**/*.{ts,tsx,js,jsx,md,mdx}'",
    "check:all": "concurrently --kill-others-on-fail --names \"check:type,check:eslint,check:prettier\" \"npm run check:type\" \"npm run check:eslint\" \"npm run check:prettier\" \"npm run check:cspell\"",
    "format:eslint": "eslint . --fix --cache --cache-location=./.cache/.eslintcache",
    "format:prettier": "prettier . --write --cache --cache-location=./.cache/.prettiercache",
    "format:all": "concurrently --kill-others-on-fail --names \"format:eslint,format:prettier\" \"npm run format:eslint\" \"npm run format:prettier\"",
    "clean-cache": "rm -rf node_modules/.cache .cache"
  },
  "dependencies": {
    "@iconify/react": "^5.2.1",
    "@prisma/client": "^6.15.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@trpc/client": "^11.5.0",
    "@trpc/server": "^11.5.0",
    "@trpc/tanstack-react-query": "^11.5.0",
    "@xyflow/react": "^12.8.4",
    "dayjs": "^1.11.15",
    "i18next": "^25.4.2",
    "motion": "^12.23.12",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "venomous-ui-react": "github:BlaxBerry333/venomous-ui-react",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@hookform/devtools": "^4.4.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "cspell": "^9.2.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.2",
    "prettier": "^3.6.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-style-order": "^0.2.2",
    "prisma": "^6.15.0",
    "typescript": "^5"
  }
}
